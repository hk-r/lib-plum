    "keywords": ["plum"],
    "license": "MIT",
    "authors": [
        {
            "name": "Kyota Hiyoshi",
            "email": "hiyoshi-kyota@imjp.co.jp"
        }
    ],
    "scripts":{
        "start": [
            "php -S 127.0.0.1:8088 -t ./"
        ],
        "preview": [
            "open http://127.0.0.1:8088/tests/testdata/htdocs/index.php"
        ],
        "test": [
            "php ./vendor/phpunit/phpunit/phpunit"
        ]
    },
    "config": {
        "process-timeout": 0
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.1.0",
        "tomk79/filesystem": "^1.0.0"
    },
    "autoload": {
        "files": [
            "php/main.php",
            "php/plum_deploy.php",
            "php/plum_git.php"
        ]
    },
    "extra": {
        "px2package": {
            "name": "lib-plum",
            "type": "plugin"
        }
    }
}
